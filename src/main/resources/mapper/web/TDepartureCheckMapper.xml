			<if test="vehicleLicence != null  and vehicleLicence != ''  ">#{vehicleLicence},</if>
			<if test="driver != null  and driver != ''  ">#{driver},</if>
			<if test="intimidateDate != null  ">#{intimidateDate},</if>
