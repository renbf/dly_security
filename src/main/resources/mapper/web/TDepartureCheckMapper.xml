			<if test="shiftsDate != null  ">#{shiftsDate},</if>
			<if test="shiftsNum != null  and shiftsNum != ''  ">#{shiftsNum},</if>
			<if test="shiftsType != null  and shiftsType != ''  ">#{shiftsType},</if>
