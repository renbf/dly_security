    <select id="selectTDrillRecordList" parameterType="TDrillRecord" resultMap="TDrillRecordResult">
        <include refid="selectTDrillRecordVo"/>
        <where>  
            <if test="id != null  and id != '' "> and dr.id = #{id}</if>
