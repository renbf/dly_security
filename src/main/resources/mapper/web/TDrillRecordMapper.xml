	<sql id="selectTDrillRecordVo">
        select dr.id, dr.contingency_plan, dr.exercise_content, dr.exercise_name, dr.effect_evaluation, dr.exercise_date, dr.commanding_officer, dr.exercise_type, dr.participants_num, dr.participants_levl, dr.check_in_num, dr.exercise_place, dr.exercise_file_path, dr.create_time, dr.update_time, dr.user_id, dr.meno, dr.business_id, dr.update_user_id from t_drill_record dr
