         </where>
    </select>
    
    <select id="selectTSafetyDutyById" parameterType="String" resultMap="TSafetyDutyResult">
        <include refid="selectTSafetyDutyVo"/>
        where sd.id = #{id}
    </select>
        
    <insert id="insertTSafetyDuty" parameterType="TSafetyDuty">
