    </delete>
	
    <delete id="deleteTSafetyDutyByIds" parameterType="String">
        delete from t_safety_duty where id in 
        <foreach item="id" collection="array" open="(" separator="," close=")">
            #{id}
        </foreach>
    </delete>
    
